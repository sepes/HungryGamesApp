{
    "name": "hungry-games-app",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "bootstrap": "^5.3.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "sass": "^1.93.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "ignorePatterns": [
            "src/index.js"
        ]
    },
    "sass": {
        "quietDeps": true,
        "silenceDeprecations": ["import", "legacy-js-api"]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}